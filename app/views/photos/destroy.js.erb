$('#photos').html("<%=  j render 'photos_list' %>")
<% if @gear.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@gear.price.blank? && !@gear.listing_name.blank? && !@gear.photos.blank? && !@gear.location.blank? %>
  $('#publish_button').attr('disabled', false);  
<% else %>
  $('#publish_button').attr('disabled', true);
<% end %>